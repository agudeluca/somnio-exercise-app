{
	"linter": {
		"rules": {
			"suspicious": {
				"noDebugger": "off",
				"noConsoleLog": "info"
			},
			"correctness": {
				"useExhaustiveDependencies": "off"
			},
			"complexity": {
				"noForEach": "off"
			},
			"a11y": {
				"useButtonType": "off",
				"useKeyWithClickEvents": "off"
			},
			"style": {
				"noNonNullAssertion": "off",
				"noShoutyConstants": "warn"
			}
		}
	}
}
